{
  "dependencies": {
    "archiver": "^7.0.1",
    "axios": "^1.7.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http": "^0.0.1-security",
    "json-2-csv": "^5.5.5",
    "node-schedule": "^2.1.1",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3"
  },
  "type": "module",
  "name": "webapi-middle-server",
  "version": "v0.0.1",
  "main": "app.mjs",
  "scripts": {
    "dev-start": "nodemon -r dotenv/config ./src/app.mjs dotenv_config_path=.env.dev",
    "start": "node -r dotenv/config ./src/app.mjs dotenv_config_path=.env",
    "clone-pages-win": "cd src/ && rd /s /q views && git clone -b gh-pages https://github.com/riddickalo/webapi-react.git views",
    "clone-pages": "mkdir -p src/views && rm -rf src/views/ && git clone -b gh-pages https://github.com/riddickalo/webapi-react.git src/views",
    "get-views": "mkdir -p src/views && cp -rfv ../webapi-react/build/* ./src/views/"
  },
